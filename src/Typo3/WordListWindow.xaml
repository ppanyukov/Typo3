<Window x:Class="Typo3.WordListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WordListWindow" Height="310" Width="433">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

            <ListBox x:Name="wordList" ItemsSource="{Binding}" FontSize="17">
            </ListBox>
            
    </Grid>
</Window>
